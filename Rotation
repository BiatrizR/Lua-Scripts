--[[Script at the part ]]

local rotatePart = script.Parent

--function para girar as partes

local function rotate(part)
	--definir os algulos referente aos eixos x, y e z
	part.CFrame = part.CFrame * CFrame.Angles(0, 0.08, 0)

end

--passar a funtion e qual objeto ela vai rotacionar para testar
--rotate(rotatePart)


--fazer loop para rotacionar sempre
while wait(0.016) do
	rotate(rotatePart)
end
